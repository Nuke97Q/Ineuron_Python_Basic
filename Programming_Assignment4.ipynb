    "\n",
    "def Fibo(num):\n",
    "    a=1\n",
    "    b=1\n",
    "    print(a)\n",
    "    for i in range(num):\n",
    "        c=a+b\n",
    "        print(c)\n",
    "        a=b\n",
    "        b=c\n",
