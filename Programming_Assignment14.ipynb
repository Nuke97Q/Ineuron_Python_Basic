   "source": [
    "Please write a program to compress and decompress the string \"hello world!hello world!hello world!hello world!\""
   ]
  },
  {
   "cell_type": "raw",
   "id": "5e54f1da",
   "metadata": {},
   "source": [
    "def solve(s):\n",
    "    res = \"\"\n",
    "    cnt = 1\n",
    "    for i in range(1, len(s)):\n",
    "        if s[i - 1] == s[i]:\n",
    "            cnt += 1\n",
    "        else:\n",
    "            res = res + s[i - 1]\n",
    "        if cnt > 1:\n",
    "            res += str(cnt)\n",
    "            cnt = 1\n",
    "            res = res + s[-1]\n",
    "    if cnt > 1:\n",
    "        res += str(cnt)\n",
    "    return res\n",
    "\n",
    "s = \"hello world!hello world!hello world!hello world!\"\n",
    "print(solve(s))"
   ]
  },
